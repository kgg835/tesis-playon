<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="parking_solid_ActionBar" parent="@style/Widget.Sherlock.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_parking</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_parking</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_parking</item>
        <item name="progressBarStyle">@style/parking_ProgressBar</item>
        <item name="android:background">@drawable/ab_solid_parking</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_parking</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_parking</item>
        <item name="android:progressBarStyle">@style/parking_ProgressBar</item>
    </style>

    <style name="parking_transparent_ActionBar" parent="@style/Widget.Sherlock.ActionBar">
        <item name="background">@drawable/ab_transparent_parking</item>
        <item name="progressBarStyle">@style/parking_ProgressBar</item>
        <item name="android:background">@drawable/ab_transparent_parking</item>
        <item name="android:progressBarStyle">@style/parking_ProgressBar</item>
    </style>

    <style name="parking_PopupMenu" parent="@style/Widget.Sherlock.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_parking</item>
    </style>

    <style name="parking_DropDownListView" parent="@style/Widget.Sherlock.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_parking</item>
    </style>

    <style name="parking_ActionBarTabStyle" parent="@style/Widget.Sherlock.ActionBar.TabView">
        <item name="background">@drawable/tab_indicator_ab_parking</item>
        <item name="android:background">@drawable/tab_indicator_ab_parking</item>
    </style>

    <style name="parking_DropDownNav" parent="@style/Widget.Sherlock.Spinner.DropDown.ActionBar">
        <item name="background">@drawable/spinner_background_ab_parking</item>
        <item name="android:background">@drawable/spinner_background_ab_parking</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_parking</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_parking</item>
    </style>

    <style name="parking_ProgressBar" parent="@style/Widget.Sherlock.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_parking</item>
    </style>

</resources>